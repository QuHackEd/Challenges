   "source": [
    "from pyquil import Program \n",
    "from pyquil.gates import H\n",
    "from pyquil.api import get_qc, local_qvm\n",
    "\n",
    "qc = get_qc(\"1q-qvm\")\n",
    "\n",
    "prog = Program()\n",
    "\n",
    "prog += H(0)\n",
    "\n",
    "with local_qvm():\n",
    "    print(qc.run_and_measure(prog, 10))"
   ]
  }
